        <attr name="fvBorderColor" format="color" />
        <attr name="fvBorderWidth" format="dimension" />
        <attr name="fvRadius" format="dimension" />
        <attr name="fv_left_top_radius" format="dimension" />
        <attr name="fv_right_top_radius" format="dimension" />
        <attr name="fv_right_bottom_radius" format="dimension" />
        <attr name="fv_left_bottom_radius" format="dimension" />
        <attr name="fv_fill_color" format="color" />
        <attr name="fv_stroke_color" format="color" />
